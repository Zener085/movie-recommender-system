# Auto detect text files and perform LF normalization
* text=auto
*.csv filter=lfs diff=lfs merge=lfs -text
*.data filter=lfs diff=lfs merge=lfs -text
allbut.pl filter=lfs diff=lfs merge=lfs -text
*.item filter=lfs diff=lfs merge=lfs -text
*.info filter=lfs diff=lfs merge=lfs -text
*.user filter=lfs diff=lfs merge=lfs -text
*.base filter=lfs diff=lfs merge=lfs -text
*.genre filter=lfs diff=lfs merge=lfs -text
*.occupation filter=lfs diff=lfs merge=lfs -text
*.test filter=lfs diff=lfs merge=lfs -text
